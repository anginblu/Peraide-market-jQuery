function Post(title, content){
  this.title = title;
  this.content = content;
};

$.get('https://api.github.com/users/cernanb', function(data){

  var newPost = new Post(data.title, data.content);
  Post.prototype.renderTitle = function(){
    return `<h2>'Title': ${this.title}</h2>`
  }
});

var newPost = new Post("My Post", "This is my first post.");

newPost.renderTitle()
